{
  "name": "polyfill-number-tolocalestring",
  "version": "2.0.0",
  "description": "Polyfill Number.prototype.toLocaleString() using globalize",
  "main": "lib/index.js",
  "web": "lib/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "dependencies": {
    "cldr-data": "^34.0.0",
    "globalize": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@nuxtjs/eslint-config": "^0.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "eslint": "^5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "full-icu": "^1.2.1",
    "jest": "^23.6.0"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rm -rf lib && mkdir lib && browserify ./src/index.js -o ./lib/index.js -t [ babelify ]",
    "lint": "eslint src tests",
    "prepublish": "npm run build",
    "test": "NODE_ICU_DATA=./node_modules/full-icu jest tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chengyin/polyfill-number-tolocalestring.git"
  },
  "keywords": [
    "tolocalestring",
    "globalize",
    "number",
    "polyfill",
    "i10n",
    "localization"
  ],
  "author": "pimlie <pimlie@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pimlie/polyfill-number-tolocalestring/issues"
  },
  "homepage": "https://github.com/pimlie/polyfill-number-tolocalestring#readme",
  "cldr-data-coverage": "full",
  "cldr-data-urls-filter": "(core|numbers-full)"
}
